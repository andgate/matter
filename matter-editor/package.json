{
  "name": "matter-editor",
  "version": "1.0.0",
  "description": "Editor for the Matter programming langauge",
  "main": "./main.js",
  "scripts": {
    "build": "cargo build & bsb -make-world",
    "electron": "electron public/index.html",
    "clean": "bsb -clean-world",
    "dev:bsb": "bsb -make-world -w",
    "dev:webpack": "webpack -w",
    "dev:cargo": "systemfd --no-pid -s http::3000 -- cargo watch -x run",
    "prod:webpack": "NODE_ENV=production webpack"
  },
  "repository": "https://github.com/andgate/matter",
  "keywords": [
    "Electron",
    "language",
    "editor"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "uglifyjs-webpack-plugin": "^1.2.5"
  },
  "dependencies": {
    "@glennsl/bs-json": "1.3.2",
    "bs-axios": "0.0.36",
    "bs-css": "^6.7.0",
    "bs-loader": "^2.0.4",
    "bs-platform": "^3.1.5",
    "bs-webapi": "0.9.1",
    "electron": "^2.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "moment": "2.22.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "reason-react": "^0.4.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.6",
    "webpack-dev-server": "^3.1.4",
    "webpack-html-plugin": "^0.1.1"
  }
}
